{
    "family": "dev_food-recommendation",
    "containerDefinitions": [
        {
            "image": "__REPO_DOMAIN__/__REPO_URL__@__IMAGE_DIGEST__",
            "name": "__ENV___food-recommendation",
            "cpu": 1024,
            "memoryReservation": __MEM__,
            "environment": [
                {
                    "name": "PROJECT",
                    "value": "food-recommendation"
                },
                {
                    "name": "PORT",
                    "value": "80"
                },
                {
                    "name": "ENV",
                    "value": "__ENV__"
                },
                {
                    "name": "REGION",
                    "value": "__REGION__"
                },
                {
                    "name": "IS_LOCAL",
                    "value": "false"
                },
                {
                    "name": "MYSQL_USER",
                    "value": "food"
                },
                {
                    "name": "MYSQL_PASSWORD",
                    "value": "asdasd123"
                },
                {
                    "name": "MYSQL_HOST",
                    "value": "devstg_mongodb.internal_ap-northeast-2"
                },
                {
                    "name": "MYSQL_PORT",
                    "value": "3306"
                },
                {
                    "name": "MYSQL_DATABASE",
                    "value": "dev_food"
                },
                {
                    "name":"REDIS_USER",
                    "value":"food"
                },
                {
                    "name":"REDIS_PASSWORD",
                    "value":"asdasd123"
                },
                {
                    "name":"REDIS_HOST",
                    "value":"devstg_redis.internal_ap-northeast-2"
                },
                {
                    "name":"REDIS_PORT",
                    "value":"6379"
                },
                {
                    "name":"REDIS_DATABASE",   
                    "value":"0"
                }
            ],
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs-idea-__ENV___food-recommendation",
                    "awslogs-region": "__REGION__"
                }
            },
            "ulimits": [
                {
                    "name": "nofile",
                    "softLimit": 65535,
                    "hardLimit": 65535
                }
            ],
            "linuxParameters": {
                "maxSwap": 0,
                "swappiness": 0
            },
            "mountPoints": [],
            "volumesFrom": [],
            "essential": true,
            "disableNetworking": false,
            "privileged": false
        },
        {
            "image": "grafana/promtail:2.3.0", 
            "name": "__ENV___promtail",
            "cpu": 256,
            "memoryReservation": 128,
            "environment": [],
            "secrets": [
                {
                    "name": "LOG_SERVER_IP",
                    "valueFrom": "arn:aws:ssm:__REGION__:730335282594:parameter/dev_food-recommendation_mysql_host"
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "log-volume", 
                    "containerPath": "/logs"
                }
            ],
            "command": [
                "-config.file=/etc/promtail/promtail-config.yaml" 
            ],
            "essential": false,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs-idea-__ENV___promtail",
                    "awslogs-region": "__REGION__"
                }
            }
        }
    ],
    "taskRoleArn": "arn:aws:iam::730335282594:role/dev_food-recommendation_default_task_role",
    "executionRoleArn": "arn:aws:iam::730335282594:role/dev_food-recommendation_default_task_execution_role",
    "networkMode": "awsvpc",
    "volumes": [
        {
            "name": "log-volume",
            "host": {
                "sourcePath": "/log"
            }
        }
    ],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "EC2"
    ]
}